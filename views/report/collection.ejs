<div class="page-breadcrumb">
    <div class="row">
        <div class="col-7 align-self-center">
            <h4 class="page-title text-truncate text-dark font-weight-medium mb-1"><%= header %></h4>
            <div class="d-flex align-items-center">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb m-0 p-0">
                        <li class="breadcrumb-item"><a href="#" class="text-muted">Home</a></li>
                        <!-- <li class="breadcrumb-item text-muted active" aria-current="page">Library</li> -->
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>

<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <% if(message){ %>
                <div class="col-md-12">
                    <div class="alert alert-<%= message.type %> alert-dismissible bg-<%= message.type %> text-white border-0 fade show"
                        role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        <%= message.message %>
                    </div>
                </div>
            <% } %>
            <div class="card">
                <div class="card-body">
                    <form action="/report" method="post">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="frm_dt">From Date</label>
                                    <input type="date" id="frm_dt" name="frm_dt" class="form-control" value="<%= selected.frm_dt %>">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="to_dt">To Date</label>
                                    <input type="date" id="to_dt" name="to_dt" class="form-control" value="<%= selected.to_dt %>">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label for="">&nbsp;</label>
                                    <button type="submit" class="btn btn-primary form-control">Search</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <% if(selected.active_flag){ %>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table id="default_order" class="table table-striped table-bordered display no-wrap"
                                style="width:100%">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Dc No.</th>
                                        <th>Entry Date</th>
                                        <th>Net Qty</th>
                                        <th>Net Weight</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(col_list.length > 0){
                                        var i = 1
                                        col_list.forEach(dt => { %>
                                            <tr>
                                                <td><%= i %></td>
                                                <td><%= dt.dc_no %></td>
                                                <td><%= dt.entry_date %></td>
                                                <td><%= dt.net_qty %></td>
                                                <td><%= dt.net_weight %></td>
                                                <td>
                                                    <button type="button" class="btn btn-secondary float-right mb-4" onclick="routeToNextPage('/user/manage', {id: '<%= dt.ac_no %>'}, true)">
                                                        <i class="fa fa-eye-alt mr-2"></i>View
                                                    </button>
                                                </td>
                                            </tr>
                                    <% })
                                    } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                <% } %>
            </div>
        </div>
    </div>
</div>